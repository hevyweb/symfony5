{% extends 'base.html.twig' %}

{% block content %}
    <h2>{{ title }}</h2>
    <div id="role-form">
        <div class="product-body">
            {{ form_start(form) }}
            {{ form_errors(form) }}
            {{ form_row(form.name) }}
            {{ form_row(form.permissions) }}
            <button type="submit">{{ submit }}</button>
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}